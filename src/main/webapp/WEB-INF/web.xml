  <filter>
    <filter-name>AdminFilter</filter-name>
    <filter-class>com.rongyifu.mms.filter.AdminFilter</filter-class>
  </filter>
  <filter>
    <filter-name>UserFilter</filter-name>
    <filter-class>com.rongyifu.mms.filter.UserFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>AdminFilter</filter-name>
    <url-pattern>/admin/jsp/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>UserFilter</filter-name>
    <url-pattern>/mer/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>proxoolServlet</servlet-name>
    <servlet-class>com.rongyifu.mms.common.ProxoolServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>dwr-invoker</servlet-name>
    <servlet-class>org.directwebremoting.servlet.DwrServlet</servlet-class>
    <init-param>
      <param-name>debug</param-name>
      <param-value>false</param-value>
    </init-param>
    <init-param>
      <param-name>allowScriptTagRemoting</param-name>
      <param-value>true</param-value>
    </init-param>
    <init-param>
      <param-name>org.directwebremoting.extend.Remoter</param-name>
      <param-value>com.rongyifu.mms.filter.MyDWRRemoter</param-value>
    </init-param>
  </servlet>
  <servlet>
    <servlet-name>CheckCode</servlet-name>
    <servlet-class>com.rongyifu.mms.servlet.CheckCode</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>go</servlet-name>
    <servlet-class>com.rongyifu.mms.ewp.Invoker</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>RecvPayResult</servlet-name>
    <servlet-class>com.rongyifu.mms.ewp.RecvPayResult</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>test</servlet-name>
    <servlet-class>com.rongyifu.mms.ewp.test</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ReconciliationInterface</servlet-name>
    <servlet-class>com.rongyifu.mms.servlet.ReconciliationInterface</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>HandlerPic</servlet-name>
    <servlet-class>com.rongyifu.mms.servlet.HandlerPic</servlet-class>
  </servlet>
   <!-- RefreshMinfoAndGateConfig -->
  <servlet>
   <servlet-name>RefreshMinfoAndGateConfig</servlet-name>
   <servlet-class>com.rongyifu.mms.ewp.RefreshMinfoAndGateConfig</servlet-class>
  </servlet>
