  </servlet> -->
  <!-- GetSuperBankId -->
   <servlet> 
	  <servlet-name>GetSuperBankId</servlet-name>
	  <servlet-class>com.rongyifu.mms.servlet.GetSuperBankId</servlet-class>
	</servlet>
  <servlet>
  	<servlet-name>uploadFile</servlet-name>
  	<servlet-class>com.rongyifu.mms.servlet.UploadFileServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>monitorApi</servlet-name>
  	<servlet-class>com.rongyifu.mms.servlet.MonitorApi</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>out</servlet-name>
  	<servlet-class>com.rongyifu.mms.servlet.OutServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>in</servlet-name>
  	<servlet-class>com.rongyifu.mms.servlet.InServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>remoteService</servlet-name>
    <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
    <init-param>   
		<param-name>contextConfigLocation</param-name>   
		<param-value>classpath:rmi_service_context.xml</param-value>
	</init-param>   
    <load-on-startup>2</load-on-startup>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>remoteService</servlet-name>
    <url-pattern>/rmi/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>monitorApi</servlet-name>
  	<url-pattern>/monitorApi</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>go</servlet-name>
    <url-pattern>/go</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>dwr-invoker</servlet-name>
    <url-pattern>/dwr/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CheckCode</servlet-name>
    <url-pattern>/CheckCode</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RecvPayResult</servlet-name>
    <url-pattern>/payret</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>test</servlet-name>
    <url-pattern>/test</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ReconciliationInterface</servlet-name>
    <url-pattern>/ryf/recoInte</url-pattern>
